        "background": "#fccffc",
        "snaptogrid": false,
        "saveWidgets": true,
        "consoleText": "",
        "lockRos": false,
        "heartbeat": "",
        "useUDPVideo": false,
        "useUDPRos": false,
        "notes": ""
    }
